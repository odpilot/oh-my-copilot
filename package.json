{
  "name": "oh-my-copilot",
  "version": "0.1.0",
  "description": "A multi-agent system built with GitHub Copilot SDK, inspired by oh-my-claudecode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "omc": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli/index.js",
    "web": "node dist/web/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "copilot",
    "ai",
    "agents",
    "automation",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "commander": "^11.0.0",
    "express": "^4.18.0",
    "ws": "^8.14.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "inquirer": "^9.0.0",
    "dotenv": "^16.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "tsup": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
